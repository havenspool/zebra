--
-- 表的结构 `heroes_data`
--

CREATE TABLE IF NOT EXISTS `heroes_data` (
  `platform` int(8) NOT NULL,
  `server_id` int(8) NOT NULL,
  `reg_date` int(15) NOT NULL DEFAULT '0',
  `reg` int(8) NOT NULL DEFAULT '0',
  `login_date` int(15) NOT NULL DEFAULT '0',
  `login` int(8) NOT NULL DEFAULT '0'
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='角色注册留存表';

